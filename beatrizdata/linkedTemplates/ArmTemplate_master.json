{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "beatrizdata"
		},
		"MySqlBeatrizSakila_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlBeatrizSakila'"
		},
		"MySql1Carloca_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySql1Carloca'"
		},
		"AzureDataLakeStoragebeatriz_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStoragebeatriz'"
		},
		"AzureDataLakeStoragebeatriz_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stabeatriz.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "beatrizdata_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MySqlBeatrizSakila_connectionString": {
						"value": "[parameters('MySqlBeatrizSakila_connectionString')]"
					},
					"AzureDataLakeStoragebeatriz_accountKey": {
						"value": "[parameters('AzureDataLakeStoragebeatriz_accountKey')]"
					},
					"MySql1Carloca_connectionString": {
						"value": "[parameters('MySql1Carloca_connectionString')]"
					},
					"AzureDataLakeStoragebeatriz_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragebeatriz_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}